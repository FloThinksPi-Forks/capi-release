  --max-time <%= p('cc.api_health_check_timeout_per_retry') %> \
  --retry-max-time <%= p('cc.api_health_check_total_timeout') %> \
  --retry 100000 \
  --retry-delay 1 \
  http://localhost:<%= p('cc.external_port') %>/v2/info

